@model IEnumerable<AdvertisingCompany.Models.AdditionalServise>

@{
    Layout = "_Layout";
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.NameAdditionalServise)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DiscriptionAdditionalServise)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NameAdditionalServise)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiscriptionAdditionalServise)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AdditionalServiseID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AdditionalServiseID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AdditionalServiseID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
